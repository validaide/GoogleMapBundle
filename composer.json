{
    "name": "validaide/googlemapbundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle for Google Map API v3 integration - PHP >= 7.0 Symfony = 3.4",
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.4.13"
        }
    },
    "keywords": [
        "google",
        "map",
        "validaide"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        },
        {
            "name": "Mark Bijl",
            "email": "mark.bijl@validaide.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "symfony/serializer": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "validaide/common": "dev-chore/symfony-4 as 4.5.1",
        "validaide/google-map": "dev-chore/symfony-4 as 4.5.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "php-http/cache-plugin": "^1.4",
        "php-http/guzzle6-adapter": "^1.0",
        "phpunit/phpunit": "^8.0",
        "phpunit/phpunit-selenium": "8.*",
        "rector/rector": "^0.11",
        "symfony/cache": "^4.4",
        "symfony/form": "^4.4",
        "symfony/phpunit-bridge": "^4.4",
        "symfony/templating": "^4.4",
        "symfony/yaml": "^4.4",
        "twig/twig": "^2 | ^3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/validaide/common.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/validaide/google-map.git",
            "no-api": true
        }
    ],
    "autoload": {
        "psr-4": {
            "Ivory\\GoogleMapBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ivory\\Tests\\GoogleMap\\": "vendor/validaide/google-map/tests",
            "Ivory\\GoogleMapBundle\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
