{
    "name": "validaide/googlemapbundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle for Google Map API v3 integration - PHP >= 7.0 Symfony = 3.4",
    "keywords": [
        "google",
        "map",
        "validaide"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        },
        {
            "name": "Mark Bijl",
            "email": "mark.bijl@validaide.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/framework-bundle": "3.4.*",
        "validaide/google-map": "3.0.*"
    },
    "require-dev": {
        "egeloen/serializer-bundle": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "php-http/cache-plugin": "^1.4",
        "php-http/guzzle6-adapter": "^1.0",
        "phpunit/phpunit": "^7.0",
        "phpunit/phpunit-selenium": "7.*",
        "symfony/cache": "3.4.*",
        "symfony/form": "3.4.*",
        "symfony/phpunit-bridge": "3.4.*",
        "symfony/templating": "3.4.*",
        "symfony/yaml": "3.4.*",
        "twig/twig": "^1.18"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/validaide/google-map.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ivory\\GoogleMapBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ivory\\Tests\\GoogleMap\\": "vendor/validaide/google-map/tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
