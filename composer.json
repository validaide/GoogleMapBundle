{
    "name": "validaide/googlemapbundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle for Google Map API v3 integration - PHP >= 8.1 Symfony = 4.4",
    "config": {
        "sort-packages": true
    },
    "keywords": [
        "google",
        "map",
        "validaide"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        },
        {
            "name": "Mark Bijl",
            "email": "mark.bijl@validaide.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^5.4",
        "symfony/serializer": "^5.4",
        "validaide/common": "dev-chore/184011118_symfony5 as 5.8.0",
        "validaide/google-map": "dev-chore/184011201_symfony5 as 5.8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2",
        "php-http/cache-plugin": "^1.7",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug": "^2.3",
        "phpunit/phpunit": "^8.0",
        "phpunit/phpunit-selenium": "8.*",
        "rector/rector": "^0.15.0",
        "symfony/cache": "^5.4",
        "symfony/form": "^5.4",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/templating": "^5.4",
        "symfony/yaml": "^5.4",
        "twig/twig": "^3.0.4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/validaide/common.git",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/validaide/google-map.git",
            "no-api": true
        }
    ],
    "autoload": {
        "psr-4": {
            "Ivory\\GoogleMapBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ivory\\Tests\\GoogleMap\\": "vendor/validaide/google-map/tests",
            "Ivory\\GoogleMapBundle\\Tests\\": "Tests/"
        }
    }
}
