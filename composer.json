{
  "name"        : "validaide/googlemapbundle",
  "type"        : "symfony-bundle",
  "description" : "Symfony Bundle for Google Map API v3 integration - PHP >= 8.1 Symfony = 4.4",
  "config"      : {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  },
  "keywords"    : [
    "google",
    "map",
    "validaide"
  ],
  "license"     : "MIT",
  "authors"     : [
    {
      "name" : "Eric GELOEN",
      "email": "geloen.eric@gmail.com"
    },
    {
      "name" : "Mark Bijl",
      "email": "mark.bijl@validaide.com"
    },
    {
      "name" : "Yakup Arslan",
      "email": "yakup.arslan@validaide.com"
    }
  ],
  "require"     : {
    "php"                         : "^8.1",
    "symfony/config"              : "*",
    "symfony/dependency-injection": "*",
    "symfony/flex"                : "^2.5",
    "symfony/http-kernel"         : "*",
    "symfony/serializer"          : "*",
    "validaide/google-map"        : "dev-VAL-2776-spike-investigate-the-steps-need-to-upgrade-to-symfony-6 as 7.6"
  },
  "require-dev" : {
    "friendsofphp/php-cs-fixer": "^3.9.5",
    "php-http/cache-plugin"    : "^1.7",
    "php-http/guzzle7-adapter" : "^1.1",
    "php-http/httplug"         : "^2.3",
    "phpunit/phpunit"          : "^8.0",
    "phpunit/phpunit-selenium" : "8.*",
    "rector/rector"            : "^0.15.0",
    "symfony/cache"            : "*",
    "symfony/form"             : "*",
    "symfony/phpunit-bridge"   : "*",
    "symfony/templating"       : "*",
    "symfony/yaml"             : "*",
    "twig/twig"                : "^3.0.4"
  },
  "repositories": [
    {
      "type"  : "vcs",
      "url"   : "https://github.com/validaide/google-map.git",
      "no-api": true
    }
  ],
  "autoload"    : {
    "psr-4": {
      "Ivory\\GoogleMapBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ivory\\Tests\\GoogleMap\\"      : "vendor/validaide/google-map/tests",
      "Ivory\\GoogleMapBundle\\Tests\\": "Tests/"
    }
  },
  "scripts"     : {
    "auto-scripts": []
  },
  "extra"       : {
    "symfony": {
      "require": "6.4.*"
    }
  }
}
